        <TextBox HorizontalAlignment="Left" Height="23" Margin="183,465,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="175" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <Label Content="Usuário: " HorizontalAlignment="Left" Margin="183,434,0,0" VerticalAlignment="Top" FontFamily="Palatino Linotype" Width="104" IsEnabled="False"/>
        <Label Content="Senha: " HorizontalAlignment="Left" Margin="405,434,0,0" VerticalAlignment="Top" FontFamily="Palatino Linotype" Width="104" IsEnabled="False"/>
        <Label Content="Confirmar Senha: " HorizontalAlignment="Left" Margin="617,434,0,0" VerticalAlignment="Top" FontFamily="Palatino Linotype" Width="104" IsEnabled="False"/>
        <RadioButton Content="Sim" HorizontalAlignment="Left" Margin="192,263,0,0" VerticalAlignment="Top"/>
        <RadioButton Content="Não" HorizontalAlignment="Left" Margin="283,263,0,0" VerticalAlignment="Top"/>
        <Label Content="Possui por alguma experiência de furto ou roubo? " HorizontalAlignment="Left" Margin="183,222,0,0" VerticalAlignment="Top" FontFamily="Palatino Linotype" Width="290" IsEnabled="False"/>
        <TextBox HorizontalAlignment="Left" Height="98" Margin="183,331,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="609"/>
        <Label Content="Caso a resposta seja sim, conte-nos" HorizontalAlignment="Left" Margin="183,300,0,0" VerticalAlignment="Top" FontFamily="Palatino Linotype" Width="205" IsEnabled="False"/>
    </Grid>
</Window>






    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
