        <Label x:Name="lblEstatisticasTitulo_Copy6" Content="• 46% das vítimas de ocorrências registradas são do sexo Feminino" HorizontalAlignment="Left" Margin="177,273,0,0" VerticalAlignment="Top" Height="27" Width="431" FontFamily="Arial Black" FontSize="11"/>
        <Label x:Name="lblEstatisticasTitulo_Copy7" Content="• As 3 ocorrências com maior quantidade de registros" HorizontalAlignment="Left" Margin="177,305,0,0" VerticalAlignment="Top" Height="27" Width="341" FontFamily="Arial Black" FontSize="11"/>
        <Label x:Name="lblEstatisticasTitulo_Copy8" Content="► Roubo Veicular" HorizontalAlignment="Left" Margin="188,337,0,0" VerticalAlignment="Top" Height="27" Width="120" FontFamily="Arial Black" FontSize="11"/>
        <Label x:Name="lblEstatisticasTitulo_Copy9" Content="► Roubo" HorizontalAlignment="Left" Margin="188,364,0,0" VerticalAlignment="Top" Height="27" Width="68" FontFamily="Arial Black" FontSize="11"/>
        <Label x:Name="lblEstatisticasTitulo_Copy10" Content="► Furto" HorizontalAlignment="Left" Margin="188,391,0,0" VerticalAlignment="Top" Height="27" Width="68" FontFamily="Arial Black" FontSize="11"/>
        <Label x:Name="lblEstatisticasTitulo_Copy11" Content="• 48% das ocorrências registradas NÂO POSSUEM B.O." HorizontalAlignment="Left" Margin="177,423,0,0" VerticalAlignment="Top" Height="27" Width="341" FontFamily="Arial Black" FontSize="11"/>
        <Label x:Name="lblEstatisticasTitulo_Copy12" Content="• Os 3 objetos com maior quantidade de roubos" HorizontalAlignment="Left" Margin="177,455,0,0" VerticalAlignment="Top" Height="27" Width="306" FontFamily="Arial Black" FontSize="11"/>
        <Label x:Name="lblEstatisticasTitulo_Copy13" Content="► Dinheiro" HorizontalAlignment="Left" Margin="188,487,0,0" VerticalAlignment="Top" Height="27" Width="88" FontFamily="Arial Black" FontSize="11"/>
        <Label x:Name="lblEstatisticasTitulo_Copy14" Content="► Carteira" HorizontalAlignment="Left" Margin="188,514,0,0" VerticalAlignment="Top" Height="27" Width="88" FontFamily="Arial Black" FontSize="11"/>
        <Label x:Name="lblEstatisticasTitulo_Copy15" Content="► Documentos" HorizontalAlignment="Left" Margin="188,541,0,0" VerticalAlignment="Top" Height="27" Width="105" FontFamily="Arial Black" FontSize="11"/>

    </Grid>
</Window>
